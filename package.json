{
  "name": "arduino-ffat-upload",
  "displayName": "ESP32 FFAT Filesystem Uploader",
  "description": "Upload files from the 'data' folder to ESP32 FFAT (FAT) filesystem partition directly from Arduino IDE 2. Supports ESP32/ESP32-S2/ESP32-S3/ESP32-C3 with auto-detection of partition layout, mkfatfs tool, and serial port.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.82.0"
  },
  "license": "MIT",
  "publisher": "arduino-ffat",
  "repository": {
    "url": "git+https://github.com/r-iki/arduino-ffat-upload.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "arduino",
    "esp32",
    "esp32s2",
    "esp32s3",
    "esp32c3",
    "ffat",
    "fat",
    "filesystem",
    "upload",
    "flash",
    "partition",
    "webserver"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "arduino-ffat-upload.uploadFFAT",
        "title": "Upload FFAT to ESP32"
      },
      {
        "command": "arduino-ffat-upload.buildFFAT",
        "title": "Build FFAT image in sketch directory"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "npx @vscode/vsce package"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "typescript": "^5.1.6",
    "vscode-arduino-api": "ide2"
  },
  "dependencies": {},
  "bugs": {
    "url": "https://github.com/r-iki/arduino-ffat-upload/issues"
  },
  "homepage": "https://github.com/r-iki/arduino-ffat-upload#readme",
  "author": "riki"
}
